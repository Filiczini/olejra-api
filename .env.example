# Database connection (PostgreSQL via Prisma)Ñ‡ 
DATABASE_URL="postgresql://USER:PASSWORD@HOST:5432/DBNAME?schema=public"

# Runtime environment: set to 'development' locally, Render/Prod will set 'production'
NODE_ENV=development
PORT=5174
CORS_ORIGIN=http://localhost:5173

# Bootstrap admin credentials (for seed/first run)
ADMIN_EMAIL=admin@example.com
ADMIN_PASSWORD=change_me

# JWT configuration (cookie-based auth)
JWT_SECRET=change_me
ACCESS_TTL=15m
